@model Reksa.ViewModels.POListViewModel
@{
    ViewData["Title"] = "Reject Booking & Cancel Transaction";
}
<div class="row">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="m-t-0 header-title">
                <a>Reject Booking & Cancel Transaction</a>
            </h4>
        </div>
        <div class="panel-body">

            <div class="card-box">
                <!-- Begin Button -->
                <div>
                    <div class="input-group">
                        <button id=btnRefresh class="btn btn-default waves-effect waves-light" style="margin-right:10px">
                            <span class="btn-label">
                                <i class="fa fa-refresh"></i>
                            </span>
                            Refresh
                        </button>
                        <button id=btnProcess class="btn btn-danger waves-effect waves-light" style="margin-right:10px">
                            <span class="btn-label">
                                <i class="fa fa-cogs"></i>
                            </span>
                            Reject
                        </button>
                    </div>
                </div>
                <!-- End Button -->
            </div>

            <div class="col-md-12">
                <ul class="nav nav-tabs" style="background-color:#efefef;border-style:double;">
                    <li class="active"><a data-toggle="tab" href="#tab-reject" id="A"><i class="fa fa-info-circle"></i>  Reject Booking</a></li>
                    <li><a data-toggle="tab" href="#tab-delete" id="B"><i class="fa fa-history"></i>  Delete Transaction</a></li>
                </ul>
                <div class="tab-content" style="border-style:double">
                    <div id="tab-reject" class="tab-pane fade in active">

                        <div class="row">
                            <div class="col-md-12">
                                <div id="dataGridView1"></div>
                            </div>
                        </div>

                    </div>
                    <div id="tab-delete" class="tab-pane fade">
                        <div class="row">
                            <div class="col-md-12">
                                <div id="dataGridView2"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/js/reksa.po.rejectbooking.js"></script>
<script src="~/js/reksa.po.rejectbooking.events.js"></script>