<script>
    var intMyType;
    $(document).ready(function () {
        intMyType = "B";
        SetToolbar(intMyType);
        SetControl(intMyType);

        $("#btnRefresh").click(function () {
            if ($("#ProdCode").val() == "") {
                swal("Warning", "Produk harap dipilih dulu!", "warning");
                return;
            }
            else {
                subRefresh();
            }
        });
        $("#btnNew").click(function () {
            subNew();
        });
        $("#btnEdit").click(function () {
            subUpdate();
        });
        $("#btnDelete").click(function () {
            subDelete();
        });
        $("#btnSave").click(function () {
            subSave(intMyType);
        });
        $("#btnCancel").click(function () {
            subCancel();
        });

        //change
        $("#_noGL1").change(function () {
            ValidateGL($("#_noGL1").val(), function (res) {
                $("#_descGL1").val(res);
            });
        });
        $("#_noGL2").change(function () {
            ValidateGL($("#_noGL2").val(), function (res) {
                $("#_descGL2").val(res);
            });
        });
        $("#_noGL3").change(function () {
            ValidateGL($("#_noGL3").val(), function (res) {
                $("#_descGL3").val(res);
            });
        });
        $("#_noGL4").change(function () {
            ValidateGL($("#_noGL4").val(), function (res) {
                $("#_descGL4").val(res);
            });
        });
        $("#_noGL5").change(function () {
            ValidateGL($("#_noGL5").val(), function (res) {
                $("#_descGL5").val(res);
            });
        });

        $("#_fromPercent").change(function () {
            if ($("#_fromPercent").val() > 100) {
                $("#_fromPercent").val(0);
            }
        });
            
        $("#_toPercent").change(function () {
            if ($("#_toPercent").val() > 100) {
                $("#_toPercent").val(0);
            }
        });
           

        $("#_percentGL1").change(function () {
            if ($("#_percentGL1").val() > 100) {
                $("#_percentGL1").val(0);
            }
            $("#_TotalPercentGL").val(+$("#_percentGL1").val() + +$("#_percentGL2").val() + +$("#_percentGL3").val() + +$("#_percentGL4").val() + +$("#_percentGL5").val());
        });

        $("#_percentGL2").change(function () {
            if ($("#_percentGL2").val() > 100) {
                $("#_percentGL2").val(0);
            }
            $("#_TotalPercentGL").val(+$("#_percentGL1").val() + +$("#_percentGL2").val() + +$("#_percentGL3").val() + +$("#_percentGL4").val() + +$("#_percentGL5").val());
        });

        $("#_percentGL3").change(function () {
            if ($("#_percentGL3").val() > 100) {
                $("#_percentGL3").val(0);
            }
            $("#_TotalPercentGL").val(+$("#_percentGL1").val() + +$("#_percentGL2").val() + +$("#_percentGL3").val() + +$("#_percentGL4").val() + +$("#_percentGL5").val());
        });

        $("#_percentGL4").change(function () {
            if ($("#_percentGL4").val() > 100) {
                $("#_percentGL4").val(0);
            }
            $("#_TotalPercentGL").val(+$("#_percentGL1").val() + +$("#_percentGL2").val() + +$("#_percentGL3").val() + +$("#_percentGL4").val() + +$("#_percentGL5").val());
