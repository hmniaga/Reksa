    function onRowSubsFeeSelect(e) {
        var data = this.dataItem(this.select());
        $("#_fromPercent").val(data.PercentFrom);
        $("#_toPercent").val(data.PercentTo);
        $("#_Persetujuan").val(data.MustApproveBy);
   };

