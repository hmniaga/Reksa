                                        <label for="NoGL1">Nomor GL 4</label>
                                        <div class="input-group m-t-10">
                                            @Html.Kendo().TextBox().Name("_namaGL4").HtmlAttributes(new { @class = "form-control input-sm", @style = "width:200px" })
                                        </div>
                                    </div>
                                    <div class="form-group m-r-10">
