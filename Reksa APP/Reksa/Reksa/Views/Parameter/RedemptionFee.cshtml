@model Reksa.ViewModels.ParameterRedempFeeViewModel

@{
    ViewData["Title"] = "Redemption Fee";
}

<div class="card-box">
    <div class="row">
        <!-- Begin Buttons -->
        <div class="col-md-12">
            <div class="input-group">
                <button id="btnRefresh" class="btn btn-primary waves-effect waves-light" style="margin-right:10px">
                    <span class="btn-label">
                        <i class="fa fa-refresh"></i>
                    </span>
                    Refresh
                </button>
                <button id="btnNew" class="btn btn-default waves-effect waves-light" style="margin-right:10px">
                    <span class="btn-label">
                        <i class="fa fa-plus"></i>
                    </span>
                    New
                </button>

                <button id="btnEdit" class="btn btn-info waves-effect waves-light" style="margin-right:10px">
                    <span class="btn-label">
                        <i class="fa fa-edit"></i>
                    </span>
                    Edit
                </button>

                <button id="btnSave" style="display:none;margin-right:10px;" class="btn btn-warning waves-effect waves-light">
                    <span class="btn-label">
                        <i class="fa fa-save"></i>
                    </span>
                    Save
                </button>
                <button id="btnCancel" type="button" style="display:none;margin-right:10px;" class="btn btn-pink waves-effect waves-light">
                    <span class="btn-label">
                        <i class="fa fa-remove"></i>
                    </span>
                    Cancel
                </button>
            </div>
        </div>
        <!-- End Buttons -->
    </div>
</div>
<div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="m-t-0 header-title">
            <a>Redemption Fee</a>
        </h4>
    </div>
    <div id="collapse2" class="panel-collapse collapse in" aria-expanded="true">
        <div class="panel-body">
            <div class="col-md-12">
                <div class="form-group">
                    <label class="col-sm-1 control-label">Produk</label>
                    <div class="input-group col-md-6">
                        @Html.Kendo().TextBoxFor(model => model.ProductModel.ProdCode).HtmlAttributes(new { @id = "ProdCode", @class = "form-control input-sm" }).Enable(false)
                        <span class="input-group-btn">
                            <a id="cmpsrProduct" href="@Url.Action("SearchProduct", "Global", new { seq = 1, type = "MB" })" data-toggle="modal" data-target="#ProductModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                        </span>
                        @Html.Kendo().TextBoxFor(model => model.ProductModel.ProdName).HtmlAttributes(new { @id = "ProdName", @class = "form-control input-sm col-md-3", style = "width: 450px;" }).Enable(false)
                        @Html.Kendo().TextBoxFor(model => model.ProductModel.ProdId).HtmlAttributes(new { @id = "ProdId", @class = "form-control input-sm", @type = "Hidden" })

                    </div>
                </div>
            </div>
            <!-- Begin Tabs -->
            <div class="col-md-12" style="padding-top:20px">
                <ul class="nav nav-tabs" style="background-color:#efefef;border-style:double;">
                    <li class="active"><a data-toggle="tab" href="#tab-redempfee" id="redempfee">Redemption Fee</a></li>
                    <li><a data-toggle="tab" href="#tab-percentfee" id="percentfee">Percentage Tiering</a></li>
                    <li><a data-toggle="tab" href="#tab-tiering" id="tiering">Tiering Notification</a></li>
                    <li><a data-toggle="tab" href="#tab-settingGL" id="settingGL">Setting GL</a></li>
                </ul>
                <div class="tab-content" style="border-style:double">
                    <!-- Begin Redemption Fee -->
                    <div id="tab-redempfee" class="tab-pane fade in active">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-heading bg-custom">Percentage Non Karyawan</div>
                                    <div class="portlet-body">
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">Minimum % Fee</label>
                                                <div class="input-group">
                                                    @Html.Kendo().NumericTextBox().Name("_txtMinPctNonKary").HtmlAttributes(new { @id = "_txtMinPctNonKary" }).Min(0).Decimals(2).Enable(false).Events(e => { e.Change("onChange_txtMinPctNonKary").Spin("onSpin_txtMinPctNonKary"); })
                                                    <span class="input-group-btn"></span>
                                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">Maksimum % Fee</label>
                                                <div class="input-group">
                                                    @Html.Kendo().NumericTextBox().Name("_txtMaxPctNonKary").HtmlAttributes(new { @id = "_txtMaxPctNonKary" }).Min(0).Decimals(2).Enable(false).Events(e => { e.Change("onChange_txtMaxPctNonKary").Spin("onSpin_txtMaxPctNonKary"); })
                                                    <span class="input-group-btn"></span>
                                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-heading bg-custom">Percentage Karyawan</div>
                                    <div class="portlet-body">
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">Minimum % Fee</label>
                                                <div class="input-group">
                                                    @Html.Kendo().NumericTextBox().Name("_txtMinPctKary").HtmlAttributes(new { @id = "_txtMinPctKary" }).Min(0).Decimals(2).Enable(false).Events(e => { e.Change("onChange_txtMinPctKary").Spin("onSpin_txtMinPctKary"); })
                                                    <span class="input-group-btn"></span>
                                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">Maksimum % Fee</label>
                                                <div class="input-group">
                                                    @Html.Kendo().NumericTextBox().Name("_txtMaxPctKary").HtmlAttributes(new { @id = "_txtMaxPctKary" }).Min(0).Decimals(2).Enable(false).Events(e => { e.Change("onChange_txtMaxPctKary").Spin("onSpin_txtMaxPctKary"); })
                                                    <span class="input-group-btn"></span>
                                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-heading bg-custom">Pencairan</div>
                                    <div class="portlet-body">
                                        <div class="row">
                                            <div class="col-md-12" style="padding-top:3%">
                                                <h3 class="panel-title">Apakah pada saat pencairan, apakah fee ikut dibagikan ?</h3>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="radio radio-danger radio-inline">
                                                    <input type="radio" id="_YesRadio" value="1" name="radioInline" checked>
                                                    <label for="_YesRadio">  Ya </label>
                                                </div>
                                                <div class="radio radio-danger radio-inline">
                                                    <input type="radio" id="_noRadio" value="0" name="radioInline">
                                                    <label for="_noRadio"> Tidak </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-heading bg-custom">Non Flat/ Flat Dalam Bulan</div>
                                    <div class="portlet-body">
                                        <div class="row">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="radio radio-danger radio-inline">
                                                        <input type="radio" id="_flatCheck" value="1" name="radioFlat" checked>
                                                        <label for="_flatCheck">  Flat </label>
                                                    </div>
                                                    <div class="radio radio-danger radio-inline">
                                                        <input type="radio" id="_nonFlatCheck" value="0" name="radioFlat">
                                                        <label for="_nonFlatCheck"> Non Flat </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="input-group">
                                                    @Html.Kendo().TextBox().Name("_bulanNonFlat").HtmlAttributes(new { @style = "width:80px" }).Enable(false)
                                                    <span class="input-group-btn"></span>
                                                    <label>&nbsp;&nbsp;&nbsp;Bulan</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Redemption Fee -->
                    <!-- Begin Percentage Tiering -->
                    <div id="tab-percentfee" class="tab-pane fade">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Status</label>
                                    <div class="input-group">
                                        @(Html.Kendo().DropDownList().Name("_statusKaryawanCmb").BindTo(new List<SelectListItem>() { new SelectListItem() { Text = "Karyawan", Value = "1" }, new SelectListItem() { Text = "Non Karyawan", Value = "0" } }).DataTextField("Text").DataValueField("Value").Value("1"))
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Periode</label>
                                    <div class="input-group">
                                        @Html.Kendo().NumericTextBox().Name("_periodTxt").HtmlAttributes(new { @id = "_periodTxt", @style = "width:80px;" }).Min(0).Decimals(0).Enable(false).Format("{0:n0}")
                                        <span class="input-group-btn"></span>
                                        <label>&nbsp;&nbsp;&nbsp;Bulan</label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Max Redemption Fee</label>
                                    <div class="input-group m-t-10">
                                        @Html.Kendo().NumericTextBox().Name("_pctRedemptTier").HtmlAttributes(new { @id = "_pctRedemptTier" }).Min(0).Decimals(2).Enable(false).Events(e => { e.Change("onChange_pctRedemptTier").Spin("onSpin_pctRedemptTier"); })
                                        <span class="input-group-btn"></span>
                                        <label>&nbsp;&nbsp;&nbsp;%</label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12" style="padding-top:2%">
                                <button id="btnAddPctTier" class="btn btn-default waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-plus"></i>
                                    </span>
                                    Tambah
                                </button>
                                <button id="btnHapusPctTier" class="btn btn-danger waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-trash"></i>
                                    </span>
                                    Hapus
                                </button>
                                <button id="btnSavePctTier" class="btn btn-warning waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-save"></i>
                                    </span>
                                    Simpan
                                </button>
                                <button id="btnCancelPctTier" class="btn btn-danger waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-crosshairs"></i>
                                    </span>
                                    Batal
                                </button>
                            </div>
                            <div class="col-md-12" style="padding-top:20px;">
                                <div id="dataGridView2"></div>
                            </div>
                        </div>
                    </div>
                    <!-- End Percentage Tiering -->
                    <!-- Begin Tiering Notification -->
                    <div id="tab-tiering" class="tab-pane fade">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Dari Persentase</label>
                                    <div class="input-group">
                                        @Html.Kendo().NumericTextBox().Name("_fromPercent").HtmlAttributes(new { style = "width: 200px;" }).Min(0).Decimals(2).Enable(false).Events(e => { e.Change("onChange_fromPercent").Spin("onSpin_fromPercent"); })
                                        <span class="input-group-btn"></span>
                                        <label>&nbsp;&nbsp;&nbsp;%</label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Sampai</label>
                                    <div class="input-group">
                                        @Html.Kendo().NumericTextBox().Name("_toPercent").HtmlAttributes(new { style = "width: 200px;" }).Min(0).Decimals(2).Enable(false).Events(e => { e.Change("onChange_toPercent").Spin("onSpin_toPercent"); })
                                        <span class="input-group-btn"></span>
                                        <label>&nbsp;&nbsp;&nbsp;%</label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Persetujuan</label>
                                    <div class="input-group">
                                        @Html.Kendo().TextBox().Name("_Persetujuan").HtmlAttributes(new { @class = "form-control input-sm", @style = "width:250px" }).Enable(false)
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12" style="padding-top:2%">
                                <h3 class="panel-title">*) Tiering Notification ini hanya untuk Non Karyawan</h3>
                            </div>
                            <div class="col-md-12" style="padding-top:2%">
                                <button id="btnAdd" class="btn btn-default waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-plus"></i>
                                    </span>
                                    Tambah
                                </button>
                                <button id="btnHapus" class="btn btn-danger waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-trash"></i>
                                    </span>
                                    Hapus
                                </button>
                                <button id="btnSimpan" class="btn btn-warning waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-save"></i>
                                    </span>
                                    Simpan
                                </button>
                                <button id="btnBatal" class="btn btn-danger waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-crosshairs"></i>
                                    </span>
                                    Batal
                                </button>
                            </div>
                            <div class="col-md-12" style="padding-top:20px;">
                                <div id="dataGridView1"></div>
                            </div>
                        </div>
                    </div>
                    <!-- End Tiering Notification -->
                    <!-- Begin Setting GL -->
                    <div id="tab-settingGL" class="tab-pane fade">
                        <div class="row">
                            <div class="col-md-12">
                                <form class="form-inline">
                                    <div class="form-group m-r-10" style="padding-left:150px">
                                        <label>Nama GL</label>
                                    </div>
                                    <div class="form-group m-r-10" style="padding-left:150px">
                                        <label>Nomor GL</label>
                                    </div>
                                    <div class="form-group m-r-10" style="padding-left:150px">
                                        <label>&nbsp;</label>
                                    </div>
                                    <div class="form-group m-r-10" style="padding-left:120px">
                                        <label>Office Id</label>
                                    </div>
                                    <div class="form-group m-r-10" style="padding-left:30px">
                                        <label>Persentase</label>
                                    </div>
                                </form>

                                <form class="form-inline">
