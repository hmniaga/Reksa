                    $("#_namaGL5").val(data.listRedempFeeGL[4].GLName);

                    $("#_noGL1").val(data.listRedempFeeGL[0].GLNumber);
                    $("#_noGL2").val(data.listRedempFeeGL[1].GLNumber);
                    $("#_noGL3").val(data.listRedempFeeGL[2].GLNumber);
