
@model Reksa.ViewModels.ParameterListViewModel

@{
    ViewData["Title"] = "Pindah Cabang";
}

<div class="row">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="m-t-0 header-title">
                <a>Pindah Cabang</a>
            </h4>
        </div>
        <div class="panel-body">
            <div class="col-md-12">
                <div class="card-box">
                    <div class="row">
                        <!-- Begin Button -->
                        <div>
                            <div class="input-group">
                                <button id=btnProcess class="btn btn-info waves-effect waves-light" style="margin-right:10px">
                                    <span class="btn-label">
                                        <i class="fa fa-cogs"></i>
                                    </span>
                                    Process
                                </button>
                            </div>
                        </div>
                        <!-- End Button -->
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="card-box">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Cabang Asal</label>
                                <div class="input-group col-md-5">
                                    @Html.Kendo().TextBoxFor(model => model.OfficeModel.OfficeId).HtmlAttributes(new { @id = "srcCabangAsal_text1", @class = "form-control input-sm" }).Enable(false)
                                    <span class="input-group-btn">
                                        <a id="srcCabangAsal" href="#display" data-toggle="modal" data-target="#OfficeModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                                    </span>
                                    @Html.Kendo().TextBoxFor(model => model.OfficeModel.OfficeName).HtmlAttributes(new { @id = "srcCabangAsal_text2", @class = "form-control input-sm", style = "width: 350px;" }).Enable(false)
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div id="dataGridAgentAsal"></div>
                        </div>
                        <div class="col-md-12" style="padding-top:20px">
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Cabang Tujuan</label>
                                <div class="input-group col-md-5">
                                    @Html.Kendo().TextBoxFor(model => model.OfficeModel.OfficeId).HtmlAttributes(new { @id = "srcCabangTujuan_text1", @class = "form-control input-sm" }).Enable(false)
                                    <span class="input-group-btn">
                                        <a id="srcCabangTujuan" href="#display" data-toggle="modal" data-target="#OfficeModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                                    </span>
                                    @Html.Kendo().TextBoxFor(model => model.OfficeModel.OfficeName).HtmlAttributes(new { @id = "srcCabangTujuan_text2", @class = "form-control input-sm", style = "width: 350px;" }).Enable(false)
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div id="dataGridAgentTujuan"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="OfficeModal" tabindex="-1" role="dialog" aria-labelledby="OfficeModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>

<script src="~/js/reksa.parameter.pindahcabang.js"></script>
<script src="~/js/reksa.parameter.pindahcabang.events.js"></script>