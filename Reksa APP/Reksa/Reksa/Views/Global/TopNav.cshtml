@model IEnumerable<Reksa.Models.NavigationModel>
<div>
    @foreach (var item in Model)
            {
    <div class="col-md-4">
        <div class="card-box">
            <div class="form-group">
                <a href="@Url.Action(item.ReportAction, item.ReportController, new { ReportCode = item.ReportCode, ReportName = item.ReportName})" class="btn btn-default">
                    <span class="btn-label">
                        <i class="fa fa-file"></i>
                    </span>
                    @item.ReportCode
                </a>
            </div>
            <h5>@item.ReportName</h5>
        </div>
    </div>

            }
</div>  