<div class="col-md-6">
    <div class="portlet">
        <div class="portlet-heading bg-custom">Switching Fee</div>
        <div class="portlet-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="col-md-5">
                        <div class="checkbox checkbox-custom checkbox-circle">
                            @Html.Kendo().CheckBox().Name("checkFeeEditSwc").HtmlAttributes(new { @class = "checkbox checkbox-info checkbox-circle" }).Label("Fee Edit").Enable(false)
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="form-group">
                        <label class="col-sm-4 control-label"></label>
                        <div class="input-group">
                            @*@Html.Kendo().TextBox().Name("MoneyFeeRedemp").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 150px;padding - top:200%;" }).Enable(false)*@
                            @Html.Kendo().NumericTextBox().Name("MoneyFeeSwc").HtmlAttributes(new { style = "width: 200px;" }).Enable(false).Min(0)
                            <span class="input-group-btn"></span>
                            <label id="labelFeeCurrencySwc">&nbsp;&nbsp;&nbsp;IDR</label>
                            <span class="input-group-btn"></span>
                            @*@Html.Kendo().TextBox().Name("PercentageFeeRedemp").HtmlAttributes(new { @class = "form-control input-sm", @style = "width:50px" }).Enable(false)*@
                            @Html.Kendo().NumericTextBox().Name("PercentageFeeSwc").HtmlAttributes(new { style = "width: 110px;" }).Enable(false).Min(0)
                            <span class="input-group-btn"></span>
                            <label id="_KeteranganFeeSwc">&nbsp;&nbsp;&nbsp;%</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="col-md-12">
    <div class="col-md-8">
        <div class="form-group">
            <label class="col-sm-3 control-label">Inputter</label>
            <div class="input-group">
                @Html.Kendo().TextBox().Name("txtbInputterSwc").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 300px;" }).Enable(false)
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label"></label>
            <div class="input-group">
                <button id="btnDokumenSwc" href="@Url.Action("Document", "Global" )" data-toggle="modal" data-target="#DocumentModalSwc" class="btn btn-default waves-effect waves-light">
                    <span class="btn-label">
                        <i class="fa fa-search"></i>
                    </span>
                    Cek Dokumen
                </button>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">Seller</label>
            <div class="input-group col-md-6">
                @Html.Kendo().TextBoxFor(model => model.ReferentorModel.EMPLID).HtmlAttributes(new { @id = "srcSellerSwc_text1", @class = "form-control input-sm", style = "width:150%;" }).Enable(false)
                <span class="input-group-btn">
                    <a id="srcSellerSwc" href="@Url.Action("SearchSellerSwc", "Global")" data-toggle="modal" data-target="#SellerSwcModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                </span>
                @Html.Kendo().TextBoxFor(model => model.ReferentorModel.NAME).HtmlAttributes(new { @id = "srcSellerSwc_text2", @class = "form-control input-sm", style = "width:250px;" }).Enable(false)
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">Kode WAPERD</label>
            <div class="input-group col-md-6">
                @Html.Kendo().TextBoxFor(model => model.WaperdModel.WaperdNo).HtmlAttributes(new { @id = "srcWaperdSwc_text1", @class = "form-control input-sm", style = "width:150%;" }).Enable(false)
