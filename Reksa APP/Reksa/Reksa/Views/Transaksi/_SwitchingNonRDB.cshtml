                    <a id="srcWaperdSwc" href="@Url.Action("SearchWaperd", "Global")" data-toggle="modal" data-target="#WaperdSwcModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                </span>
                @Html.Kendo().TextBoxFor(model => model.WaperdModel.WaperdNo).HtmlAttributes(new { @id = "srcWaperdSwc_text2", @class = "form-control input-sm", style = "width:250px;" }).Enable(false)
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">Tgl Expire</label>
            <div class="input-group">
                @Html.Kendo().TextBox().Name("textExpireWaperdSwc").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 150%;" }).Enable(false)
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">NIK Referrantor</label>
            <div class="input-group col-md-6">
                @Html.Kendo().TextBoxFor(model => model.ReferentorModel.EMPLID).HtmlAttributes(new { @id = "srcReferentorSwc_text1", @class = "form-control input-sm", style = "width:150%;" }).Enable(false)
                <span class="input-group-btn">
                    <a id="srcReferentorSwc" href="@Url.Action("SearchReferentorSwc", "Global")" data-toggle="modal" data-target="#NIKRefSwcModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                </span>
                @Html.Kendo().TextBoxFor(model => model.ReferentorModel.NAME).HtmlAttributes(new { @id = "srcReferentorSwc_text2", @class = "form-control input-sm", style = "width:250px;" }).Enable(false)
            </div>
        </div>
    </div>
</div>
