<script>
    function onRowViewRDBSelect(e) {
        var data = this.dataItem(this.select());
        $("#NoTransaksiRDB").val(data.NoTrx);
        $("#ProdCodeRDB").val(data.KodeProduk);
        var DateTrx = new Date(data.TglTrx);
        $("#TglTransaksiRDB").val(DateTrx.getDate() + '/' + (DateTrx.getMonth() + 1) + '/' + DateTrx.getFullYear());
        $("#ClientCodeRDB").val(data.ClientCode);
        $('#FeeeditRDB').prop('checked', data.EditFee);
        $('#PhoneOrderRDB').prop('checked', data.PhoneOrder);
        $("#CurrCodeRDB").val(data.CCY);
        ValidateCurrCodeRDB();
        $("#NominalRDB").val(data.Nominal);
        $("#jangkawaktuRDB").val(data.JangkaWaktu);
        var dateJatuhTempo = new Date(data.JatuhTempo);
        $("#dateJatuhTempo").val(dateJatuhTempo.getDate() + '/' + (dateJatuhTempo.getMonth() + 1) + '/' + dateJatuhTempo.getFullYear());

        $("#currFeeRDB").text(data.CCY);
        $("#InputterRDB").val(data.Inputter);
        
        

        var KodeProduk = data.KodeProduk;
        var ClientCode = data.ClientCode;
        var ProdId;

        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: KodeProduk, Col2: '', Validate: 1 },
            success: function (data) {
                $("#ProdNameRDB").val(data[0].ProdName);
                ProdId = data[0].ProdId;
                $.ajax({
                    type: 'GET',
                    url: '/Global/ValidateClient',
                    data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
                    success: function (data) {
                        $("#ClientNameRDB").val(data[0].ClientName);
                    }
                });
            }
        });

        var dropdownlist = $("#JenisRDB").data("kendoDropDownList");
        var intJenis = data.JenisFee;
        //By Nominal
        if (intJenis == 0) {
            dropdownlist.select(0);
            $("#FeeRDB").val(data.NominalFee);
            $("#PercentageFeeRDB").val(data.PctFee);
            //By %
        } else {
            dropdownlist.select(1);
            $("#FeeRDB").val(data.PctFee);
            $("#PercentageFeeRDB").val(data.NominalFee);
        }
    };


