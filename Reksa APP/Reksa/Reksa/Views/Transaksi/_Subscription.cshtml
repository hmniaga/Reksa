<script type="text/javascript">
    $.ajaxSetup({ cache: false });
    function onRowViewSubsSelect(e) {
        var data = this.dataItem(this.select());
        $("#NoTransaksiSubs").val(data.NoTrx);
        $("#ProdCodeSubs").val(data.KodeProduk); 
        var DateTrx = new Date(data.TglTrx);  
        $("#TglTransaksiSubs").val(DateTrx.getDate() + '/' + (DateTrx.getMonth() + 1) + '/' + DateTrx.getFullYear());
        $("#ClientCodeSubs").val(data.ClientCode);
        $("#CurrCodeSubs").val(data.CCY);  
        $("#NominalSubs").val(data.Nominal);
        $('#FullamountSubs').prop('checked', data.FullAmount);
        $('#FeeeditSubs').prop('checked', data.EditFee);
        $('#PhoneOrderSubs').prop('checked', data.PhoneOrder);
        ValidateCurrCodeSubs();
        var KodeProduk = data.KodeProduk;
        var ClientCode = data.ClientCode;
        var ProdId;
        
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: KodeProduk , Col2: '', Validate: 1 },
            success: function (data) {
                $("#ProdNameSubs").val(data[0].ProdName);
                ProdId = data[0].ProdId;
                $.ajax({
                    type: 'GET',
                    url: '/Global/ValidateClient',
                    data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
                    success: function (data) {
                        $("#ClientNameSubs").val(data[0].ClientName);
                    }
                });
            }
        });

        var dropdownlist = $("#JenisSubs").data("kendoDropDownList");
        var intJenis = data.JenisFee;
        //By Nominal
        if (intJenis == 0) {
            dropdownlist.select(0);
            $("#FeeSubs").val(data.NominalFee);
            $("#PercentageFeeSubs").val(data.PctFee);
        //By %
        } else
        {
            dropdownlist.select(1);
            $("#FeeSubs").val(data.PctFee);
            $("#PercentageFeeSubs").val(data.NominalFee);
        }
    };

    function ValidateCurrCodeSubs() {
        var CurrCode = $("#CurrCodeSubs").val();
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateCurrency',
            data: { Col1: CurrCode, Col2: '', Validate: 1 },
            success: function (data) {
                $("#CurrNameSubs").val(data[0].CurrencyName);
            }
        });
    }

    function ValidateProdCodeSubs() {
        var ProductCode = $("#ProdCodeSubs").val();        
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: ProductCode, Col2: '', Validate: 1 },
            success: function (data) {
                $("#ProdNameSubs").val(data[0].ProdName);
                $("#ProdIdSubs").val(data[0].ProdId);
            }
        });
    }  
    function ValidateClientSubs() {
        var ClientCode = $("#ClientCodeSubs").val();
        var ProdId = $("#ProdIdSubs").val();
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateClient',
            data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
            success: function (data) {
