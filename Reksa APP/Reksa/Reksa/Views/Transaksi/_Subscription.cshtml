<script type="text/javascript">
    function onRowViewSubsSelect(e) {
        var data = this.dataItem(this.select());
        $("#textNoTransaksiSubs").val(data.NoTrx);
        $("#srcProductSubs_text1").val(data.KodeProduk); 
        var dateTglTransaksiSubs = new Date(data.TglTrx);  
        $("#dateTglTransaksiSubs").val(dateTglTransaksiSubs.getDate() + '/' + (dateTglTransaksiSubs.getMonth() + 1) + '/' + dateTglTransaksiSubs.getFullYear());
        $("#srcClientSubs_text1").val(data.ClientCode);
        $("#srcCurrencySubs_text1").val(data.CCY);  
        //$("#MoneyNomSubs").val(data.Nominal);
        $("#MoneyNomSubs").data("kendoNumericTextBox").value(data.Nominal);
        $('#checkFullAmtSubs').prop('checked', data.FullAmount);
        $('#checkFeeEditSubs').prop('checked', data.EditFee);
        $('#checkPhoneOrderSubs').prop('checked', data.PhoneOrder);
        ValidateCurrCodeSubs();
        var KodeProduk = data.KodeProduk;
        var ClientCode = data.ClientCode;
        var ProdId;
        
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: KodeProduk , Col2: '', Validate: 1 },
            success: function (data) {
                if (ClientCode != '') {
                    $("#srcProductSubs_text1").val(data[0].ProdCode);
                    $("#srcProductSubs_text2").val(data[0].ProdName);
                    ProdId = data[0].ProdId;
                    $.ajax({
                        type: 'GET',
                        url: '/Global/ValidateClient',
                        data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
                        success: function (data) {
                            $("#srcClientSubs_text2").val(data[0].ClientName);
                        }
                    });
                }
            }
        });

        var dropdownlist = $("#_ComboJenisSubs").data("kendoDropDownList");
        var intJenis = data.JenisFee;
        //By Nominal
        if (intJenis == 0) {
            dropdownlist.select(0);
            $("#MoneyFeeSubs").val(data.NominalFee);
            $("#PercentageFeeSubs").val(data.PctFee);
        //By %
        } else
        {
            dropdownlist.select(1);
            $("#MoneyFeeSubs").val(data.PctFee);
            $("#PercentageFeeSubs").val(data.NominalFee);
        }
    };
    function ValidateCurrCodeSubs() {
        var CurrCode = $("#srcCurrencySubs_text1").val();
        if (CurrCode != '') {
            $.ajax({
                type: 'GET',
                url: '/Global/ValidateCurrency',
                data: { Col1: CurrCode, Col2: '', Validate: 1 },
                success: function (data) {
                    $("#srcCurrencySubs_text2").val(data[0].CurrencyName);
                }
            });
        }
    }
    function ValidateProdCodeSubs() {
        var ProductCode = $("#srcProductSubs_text1").val();        
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: ProductCode, Col2: '', Validate: 1 },
            success: function (data) {
                $("#srcProductSubs_text2").val(data[0].ProdName);
                $("#ProdIdSubs").val(data[0].ProdId);
            }
        });
    }  
    function ValidateClientSubs() {
        var ClientCode = $("#srcClientSubs_text1").val();
        var ProdId = $("#ProdIdSubs").val();
        if (ClientCode != '') {
            $.ajax({
                type: 'GET',
                url: '/Global/ValidateClient',
                data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
                success: function (data) {
                    $("#srcClientSubs_text2").val(data[0].ClientName);
                }
            });
        }
    }  
</script>
