</div>
<div class="modal fade" id="CurrencySubsModal" tabindex="-1" role="dialog" aria-labelledby="CurrencySubsModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>
<div class="modal fade" id="SellerSubsModal" tabindex="-1" role="dialog" aria-labelledby="SellerSubsModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>
<div class="modal fade" id="WaperdSubsModal" tabindex="-1" role="dialog" aria-labelledby="WaperdSubsModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>
<div class="modal fade" id="NIKRefSubsModal" tabindex="-1" role="dialog" aria-labelledby="NIKRefSubsModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>

<div class="modal fade" id="DocumentModalSubs" tabindex="-1" role="dialog" aria-labelledby="DocumentModalSubsLabel">
    <div class="modal-dialog" style="width:75%;">
        <div class="modal-content">
            
        </div>
        </div>
</div>
