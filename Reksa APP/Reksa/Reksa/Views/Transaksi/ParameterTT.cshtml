
@model Reksa.ViewModels.TransaksiListViewModel
@{
    ViewData["Title"] = "Parameter TT Completion";
}
<div class="row">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="m-t-0 header-title">
                <a>Parameter TT Completion</a>
            </h4>
        </div>
        <div class="panel-body">
            <div class="card-box">
                <!-- Begin Button -->
                <div>
                    <div class="input-group">
                        <button id=btnRefresh class="btn btn-default waves-effect waves-light" style="margin-right:10px">
                            <span class="btn-label">
                                <i class="fa fa-refresh"></i>
                            </span>
                            Refresh
                        </button>
                        <button id="btnNew" class="btn btn-default waves-effect waves-light" style="margin-right:10px">
                            <span class="btn-label">
                                <i class="fa fa-plus"></i>
                            </span>
                            New
                        </button>
                        <button id="btnEdit" class="btn btn-info waves-effect waves-light" style="margin-right:10px">
                            <span class="btn-label">
                                <i class="fa fa-edit"></i>
                            </span>
                            Edit
                        </button>
                        <button id="btnSave" class="btn btn-warning waves-effect waves-light" style="margin-right:10px">
                            <span class="btn-label">
                                <i class="fa fa-save"></i>
                            </span>
                            Save
                        </button>
                        <button id="btnCancel" class="btn btn-pink waves-effect waves-light" style="margin-right:10px">
                            <span class="btn-label">
                                <i class="fa fa-remove"></i>
                            </span>
                            Cancel
                        </button>
                    </div>
                </div>
                <!-- End Button -->
            </div>
            <div class="col-md-12">
                <div class="card-box">
                    <div class="row">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Produk Reksadana</label>
                            <div class="input-group col-md-5">
                                @Html.Kendo().TextBoxFor(model => model.ProductModel.ProdCode).HtmlAttributes(new { @id = "_cmpsrProduct_text1", @class = "form-control input-sm" }).Enable(false)
                                <span class="input-group-btn">
                                    <a id="_cmpsrProduct" href="@Url.Action("SearchProduct", "Global", new { seq = 1, type = "MB" })" data-toggle="modal" data-target="#ProductModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                                </span>
                                @Html.Kendo().TextBoxFor(model => model.ProductModel.ProdName).HtmlAttributes(new { @id = "_cmpsrProduct_text2", @class = "form-control input-sm", style = "width: 380px;" }).Enable(false)
                                @Html.Kendo().TextBoxFor(model => model.ProductModel.ProdId).HtmlAttributes(new { @id = "ProdId", @class = "form-control input-sm", @type = "Hidden" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Currency</label>
                            <div class="input-group col-sm-2">
                                @Html.Kendo().TextBox().Name("_prodCurr").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 150px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Nama Pemohon</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtNamaPemohon").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Alamat Pemohon 1</label>
                            <div class="input-group col-sm-2">
                                @Html.Kendo().TextBox().Name("_txtAlamatPemohon1").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Alamat Pemohon 2</label>
                            <div class="input-group col-md-3">
                                @Html.Kendo().TextBox().Name("_txtAlamatPemohon2").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Nama Penerima</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtNamaPenerima").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Alamat Penerima 1</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtAlamatPenerima1").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Alamat Penerima 2</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtAlamatPenerima2").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Alamat Penerima 3</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtAlamatPenerima3").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Beneficiary Bank Code</label>
                            <div class="input-group col-md-5">
                                @Html.Kendo().TextBoxFor(model => model.BankCodeModel.KodeBank).HtmlAttributes(new { @id = "_cmpsrBeneficiaryBC_text1", @class = "form-control input-sm" }).Enable(false)
                                <span class="input-group-btn">
                                    <a id="_cmpsrBeneficiaryBC" href="@Url.Action("SearchBankCode", "Global")" data-toggle="modal" data-target="#BankCodeModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                                </span>
                                @Html.Kendo().TextBoxFor(model => model.BankCodeModel.NamaBank).HtmlAttributes(new { @id = "_cmpsrBeneficiaryBC_text2", @class = "form-control input-sm", style = "width: 380px;" }).Enable(false)
                                @Html.Kendo().TextBoxFor(model => model.BankCodeModel.Alamat).HtmlAttributes(new { @id = "BankCodeAddress", @class = "form-control input-sm", @type = "Hidden" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Beneficiary Account No</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtBeneficiaryAccNo").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Beneficiary Bank Name</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtBeneficiaryBankName").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Beneficiary Bank Address</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtBeneficiaryAddress").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 600px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Payment Remarks 1</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtRemarks1").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Payment Remarks 2</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtRemarks2").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">No Rekening Produk</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtNoRekProduk").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">No GL Biaya Full Amount</label>
                            <div class="input-group">
                                @Html.Kendo().TextBox().Name("_txtGLBiayaFullAmt").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 400px;" }).Enable(false)
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<div class="modal fade" id="ProductModal" tabindex="-1" role="dialog" aria-labelledby="ProductModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>
<div class="modal fade" id="BankCodeModal" tabindex="-1" role="dialog" aria-labelledby="BankCodeModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>
<script src="~/js/reksa.transaksi.parametertt.js"></script>
<script src="~/js/reksa.transaksi.parametertt.events.js"></script>
