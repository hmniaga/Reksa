<script>
    function onRowViewBookSelect(e) {
        var data = this.dataItem(this.select());
        $("#NoTransaksiBook").val(data.NoTrx);
        $("#srcProductBooking_text1").val(data.KodeProduk);
        var DateTrx = new Date(data.TglTrx);
        $("#dateTglTransaksiBooking").val(DateTrx.getDate() + '/' + (DateTrx.getMonth() + 1) + '/' + DateTrx.getFullYear());
        $("#srcClientBooking_text1").val(data.ClientCode);
        $("#srcCurrencyBooking_text1").val(data.CCY);


        $('#checkFeeEditBooking').prop('checked', data.EditFee);
        $('#checkPhoneOrderBooking').prop('checked', data.PhoneOrder);

        var KodeProduk = data.KodeProduk;
        var ClientCode = data.ClientCode;
        var ProdId;

        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: KodeProduk, Col2: '', Validate: 1 },
            success: function (data) {
                $("#srcProductBooking_text2").val(data[0].ProdName);
                ProdId = data[0].ProdId;
                $.ajax({
                    type: 'GET',
                    url: '/Global/ValidateClient',
                    data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
                    success: function (data) {
                        $("#srcClientBooking_text2").val(data[0].ClientName);
                    }
                });
            }
        });

        var dropdownlist = $("#_ComboJenisBooking").data("kendoDropDownList");
        var intJenis = data.JenisFee;
        //By Nominal
        if (intJenis == 0) {
            dropdownlist.select(0);
            $("#MoneyFeeBooking").val(data.NominalFee);
            $("#PercentageFeeBooking").val(data.PctFee);
            //By %
        } else {
            dropdownlist.select(1);
            $("#MoneyFeeBooking").val(data.PctFee);
            $("#PercentageFeeBooking").val(data.NominalFee);
        }
    };


    function ValidateProdCodeBook() {
        var ProductCode = $("#srcProductBooking_text1").val();
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: ProductCode, Col2: '', Validate: 1 },
            success: function (data) {
                $("#srcProductBooking_text2").val(data[0].ProdName);
                $("#ProdIdBook").val(data[0].ProdId);
            }
        });
    }
    function ValidateClientBook() {
        var ClientCode = $("#srcClientBooking_text1").val();
        var ProdId = $("#ProdIdBook").val();
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateClient',
            data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
            success: function (data) {
                $("#srcClientBooking_text2").val(data[0].ClientName);
            }
        });
    }



</script>
