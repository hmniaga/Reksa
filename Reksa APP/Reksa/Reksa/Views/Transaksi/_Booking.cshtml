<script>
    function onRowViewSwcSelect(e) {
        var data = this.dataItem(this.select());
        $("#NoTransaksiSwc").val(data.NoTrx);
        $("#ProdCodeSwc").val(data.KodeProduk);
        var DateTrx = new Date(data.TglTrx);
        $("#TglTransaksiSwc").val(DateTrx.getDate() + '/' + (DateTrx.getMonth() + 1) + '/' + DateTrx.getFullYear());
        $("#ClientCodeSwc").val(data.ClientCode);
        $("#CurrCodeSwc").val(data.CCY);


        $('#FeeeditSwc').prop('checked', data.EditFee);
        $('#PhoneOrderSwc').prop('checked', data.PhoneOrder);

        var KodeProduk = data.KodeProduk;
        var ClientCode = data.ClientCode;
        var ProdId;

        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: KodeProduk, Col2: '', Validate: 1 },
            success: function (data) {
                $("#ProdNameSwc").val(data[0].ProdName);
                ProdId = data[0].ProdId;
                $.ajax({
                    type: 'GET',
                    url: '/Global/ValidateClient',
                    data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
                    success: function (data) {
                        $("#ClientNameSwc").val(data[0].ClientName);
                    }
                });
            }
        });

        var dropdownlist = $("#JenisSwc").data("kendoDropDownList");
        var intJenis = data.JenisFee;
        //By Nominal
        if (intJenis == 0) {
            dropdownlist.select(0);
            $("#FeeSwc").val(data.NominalFee);
            $("#PercentageFeeSwc").val(data.PctFee);
            //By %
        } else {
            dropdownlist.select(1);
            $("#FeeSwc").val(data.PctFee);
            $("#PercentageFeeSwc").val(data.NominalFee);
        }
    };


    function ValidateProdCodeSwc() {
        var ProductCode = $("#ProdCodeSwc").val();
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: ProductCode, Col2: '', Validate: 1 },
            success: function (data) {
                $("#ProdNameSwc").val(data[0].ProdName);
                $("#ProdIdSwc").val(data[0].ProdId);
            }
        });
    }
    function ValidateClientSwc() {
        var ClientCode = $("#ClientCodeSwc").val();
        var ProdId = $("#ProdIdSwc").val();
        $.ajax({
            type: 'GET',
            url: '/Global/ValidateClient',
            data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
            success: function (data) {
                $("#ClientNameSwc").val(data[0].ClientName);
            }
        });
    }



</script>
