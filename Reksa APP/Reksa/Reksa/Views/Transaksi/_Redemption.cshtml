<script>
    function onRowViewRedempSelect(e) {
        var data = this.dataItem(this.select());
        $("#textNoTransaksiRedemp").val(data.NoTrx);
        $("#srcProductRedemp_text1").val(data.KodeProduk);
        var dateTglTransaksiRedemp = new Date(data.TglTrx);
        $("#dateTglTransaksiRedemp").val(dateTglTransaksiRedemp.getDate() + '/' + (dateTglTransaksiRedemp.getMonth() + 1) + '/' + dateTglTransaksiRedemp.getFullYear());
        $("#srcClientRedemp_text1").val(data.ClientCode);
        $("#CurrCodeRedemp").val(data.CCY);
        //$("#OutstandingUnitRedemp").val(data.OutstandingUnit);
        $("#OutstandingUnitRedemp").data("kendoNumericTextBox").value(data.OutstandingUnit);        
        //$("#RedempUnit").val(data.RedempUnit);
        $("#RedempUnit").data("kendoNumericTextBox").value(data.RedempUnit);
        if (data.OutstandingUnit == data.RedempUnit) {
            $('#checkAll').prop('checked', true);
        } else
        {
            $('#checkAll').prop('checked', false);
        }
        
        $('#checkFeeEditRedemp').prop('checked', data.EditFee);
        $('#checkPhoneOrderRedemp').prop('checked', data.PhoneOrder);
        
        var KodeProduk = data.KodeProduk;
        var ClientCode = data.ClientCode;
        var ProdId;

        $.ajax({
            type: 'GET',
            url: '/Global/ValidateProduct',
            data: { Col1: KodeProduk, Col2: '', Validate: 1 },
            success: function (data) {
                $("#srcProductRedemp_text2").val(data[0].ProdName);
                ProdId = data[0].ProdId;
                $.ajax({
                    type: 'GET',
                    url: '/Global/ValidateClient',
                    data: { Col1: ClientCode, Col2: '', Validate: 1, ProdId: ProdId },
                    success: function (data) {
                        $("#srcClientRedemp_text2").val(data[0].ClientName);
                    }
                });
            }
        });

        var dropdownlist = $("#_ComboJenisRedemp").data("kendoDropDownList");
        var intJenis = data.JenisFee;
        //By Nominal
        if (intJenis == 0) {
            dropdownlist.select(0);
            $("#MoneyFeeRedemp").val(data.NominalFee);
            $("#PercentageFeeRedemp").val(data.PctFee);
            //By %
        } else {
            dropdownlist.select(1);
            $("#MoneyFeeRedemp").val(data.PctFee);
            $("#PercentageFeeRedemp").val(data.NominalFee);
        }
    };


