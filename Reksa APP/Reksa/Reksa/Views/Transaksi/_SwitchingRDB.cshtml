                <span class="input-group-btn">
                    <a id="srcSellerSwcRDB" href="#display" data-toggle="modal" data-target="#SellerModal" class="btn btn-default btn-sm btn-search-component src-cif enabled">...</a>
                </span>
                @Html.Kendo().TextBoxFor(model => model.ReferentorModel.NAME).HtmlAttributes(new { @id = "srcSellerSwcRDB_text2", @class = "form-control input-sm", style = "width:250px;" }).Enable(false)
