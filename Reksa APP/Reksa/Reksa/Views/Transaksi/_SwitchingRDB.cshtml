<div class="col-md-6">
    <div class="portlet">
        <div class="portlet-heading bg-custom">Switching Fee</div>
        <div class="portlet-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="col-md-5">
                        <div class="checkbox checkbox-custom checkbox-circle">
                            @Html.Kendo().CheckBox().Name("checkFeeEditSwcRDB").HtmlAttributes(new { @class = "checkbox checkbox-info checkbox-circle" }).Label("Fee Edit").Enable(false)
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="form-group">
                        <label class="col-sm-4 control-label"></label>
                        <div class="input-group">
                            @*@Html.Kendo().TextBox().Name("MoneyFeeRedemp").HtmlAttributes(new { @class = "form-control input-sm", style = "width: 150px;padding - top:200%;" }).Enable(false)*@
                            @Html.Kendo().NumericTextBox().Name("MoneyFeeSwcRDB").HtmlAttributes(new { style = "width: 200px;" }).Enable(false).Min(0)
                            <span class="input-group-btn"></span>
                            <label id="labelFeeCurrencySwcRDB">&nbsp;&nbsp;&nbsp;CURR</label>
                            <span class="input-group-btn"></span>
                            @Html.Kendo().NumericTextBox().Name("PercentageFeeSwcRDB").HtmlAttributes(new { style = "width: 110px;" }).Enable(false).Min(0).Events(e => { e.Change("onChangePercentageFeeSwcRDB").Spin("onSpinPercentageFeeSwcRDB"); })
                            <span class="input-group-btn"></span>
                            <label id="_KeteranganFeeSwcRDB">&nbsp;&nbsp;&nbsp;%</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
