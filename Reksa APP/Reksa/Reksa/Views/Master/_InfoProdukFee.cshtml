                                    @Html.Kendo().NumericTextBox().Name("textBox15").HtmlAttributes(new { style = "width: 150px;" }).Enable(false).Min(0)
                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                </div>
                            </div>
                            <!-- End Redemption Fee -->
                            <!-- Begin Maintenance Fee -->
                            <div class="form-group">
                                <label class="col-sm-4 control-label">Maintenance Fee</label>
                                <div class="input-group">
                                    @Html.Kendo().NumericTextBox().Name("textBox16").HtmlAttributes(new { style = "width: 150px;" }).Enable(false).Min(0)
                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                </div>
                            </div>
                            <!-- End Maintenance Fee -->
                            <!-- Begin Up Front Fee -->
                            <div class="form-group">
                                <label class="col-sm-4 control-label">Up Front Fee</label>
                                <div class="input-group">
                                    @Html.Kendo().NumericTextBox().Name("textBox3").HtmlAttributes(new { style = "width: 150px;" }).Enable(false).Min(0)
                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                </div>
                            </div>
                            <!-- End Up Front Fee -->
