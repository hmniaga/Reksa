                    <div class="portlet-heading bg-custom">Fee Based</div>
                    <div class="portlet-body">
                        <div class="row">
                            <!-- Begin Subscription Fee -->
                            <div class="form-group">
                                <label class="col-sm-4 control-label">Subscription Fee</label>
                                <div class="input-group">
                                    @Html.Kendo().NumericTextBox().Name("textBox13").HtmlAttributes(new { style = "width: 150px;" }).Enable(false).Min(0)
                                    <label>&nbsp;&nbsp;&nbsp;%</label>
                                </div>
                            </div>
                            <!-- End Subscription Fee -->
                            <!-- Begin Redemption Fee -->
                            <div class="form-group">
                                <label class="col-sm-4 control-label">Redemption Fee</label>
